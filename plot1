data<-read.table("household_power_consumption.txt",sep=";",header=TRUE,na.strings = "?")
data$Date<-as.Date(HPC$Date,format="%y/%m/%d")
data$Global_active_power<-as.numberic(data$Global_active_power)
vector<-(data$Date=="2007-02-01"|data$Date=="2007-02-02")
subdata<-data[vector,]


png(filename = "Plot1.png", width = 480, height = 480, units = "px")
hist(subdata$Global_active_power,col="red",xlab="Global Active Power (kilowatts)",ylab="frequency",main="Global Active Power")

dev.off() 
